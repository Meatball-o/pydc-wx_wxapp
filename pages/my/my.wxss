.my{
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.my navigator{
  height: 88rpx;
  line-height: 88rpx;
  position: relative;
  padding-left: 60rpx;
  font-size: 32rpx;
}
.my navigator .iconfont:first-child{
  position: absolute;
  top:-2rpx;
  left: 0rpx;
  font-weight: bold;
}
.favorite .iconfont:first-child{
  font-size: 42rpx;
  color: #E98071;
}
.history .iconfont:first-child{
  font-size: 40rpx;
  color: #B2D55D;
}
.business .iconfont:first-child{
  font-size: 35rpx;
  color: #F095A3;
}
.feedback .iconfont:first-child{
  font-size: 35rpx;
  color: #8CC9BC;
}

.personal-info{
  width: 750rpx;
  height: 300rpx;
  background-color: red;
}