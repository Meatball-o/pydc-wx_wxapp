page{
  background-color: #f3f3f3;
}
.about{
  width: 100%;
  box-sizing: border-box;
  padding:  20rpx 30rpx;
}
.about text:first-child{
  font-weight: bold;
  padding: 10rpx 0;
  color: #000;
}
.about text:nth-child(2){
  display: block;
  color: #aaa;
  padding: 20rpx 0
}
.about view{
  background-color: #fff;
  color: #000;
  padding: 20rpx;
  text-indent:56rpx;
  line-height: 56rpx;
  border-radius: 4rpx;
}